    "mysql2": "^3.14.4",
    "nodemailer": "^7.0.6",
    "pg": "^8.16.3",
    "protobuf": "link:@bufbuild/protobuf",
    "sequelize": "^6.37.7",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0"
  },
